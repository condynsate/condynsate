<?xml version="1.0" ?>
<robot name="gyro">


    <!-- ########### MATERIALS ########### -->

    <material name="industrial">
        <color rgba="0.114 0.345 0.655 1" />
    </material>
    
    <material name="arches">
        <color rgba="0 0.624 0.831 0.333" />
    </material>
    
    <material name = "heritage">
        <color rgba = "0.961 0.510 0.118 1" />
    </material>
  

    <!-- ########### LINKS ########### -->
    <link name="chassis" concave="yes">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="spacecraft_chassis_1OD_0.9ID.stl" scale="2 2 2"/>
            </geometry>
            <material name = "arches"/>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="3.06"/>
            <inertia ixx="0.0185" ixy="0" ixz="0" iyy="0.0185" iyz="0" izz="0.0185"/>
        </inertial>
    </link>

    <link name="outer_gimbal" concave="yes">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="gyroring_0.9-ODx0.6-ID_center_origin.stl" scale="2 2 2"/>
            </geometry>
            <material name = "industrial"/>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.221"/>
            <inertia ixx="0.000478" ixy="0" ixz="0" iyy="0.000478" iyz="0" izz="0.000939"/>
        </inertial>
    </link>

    <link name="inner_gimbal" concave="yes">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="gyroring_0.6-ODx0.3-ID_center_origin.stl" scale="2 2 2"/>
            </geometry>
            <material name = "industrial"/>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.119"/>
            <inertia ixx="0.000149" ixy="0" ixz="0" iyy="0.0000783" iyz="0" izz="0.0000783"/>
        </inertial>
    </link>

    <link name="core">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="gyrocore.stl" scale="2 2 2"/>
            </geometry>
            <material name = "heritage"/>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.343"/>
            <inertia ixx="0.000107" ixy="0" ixz="0" iyy="0.0000650" iyz="0" izz="0.0000650"/>
        </inertial>
    </link>


    <!-- ########### JOINTS ########### -->
    <joint name="chassis_to_outer" type="continuous">
        <parent link = "chassis" />
        <child link = "outer_gimbal" />
        <origin rpy="0 0 0" xyz = "0 0 0" />
        <axis xyz = "1 0 0" />
    </joint>

    <joint name="outer_to_inner" type="continuous">
        <parent link = "outer_gimbal" />
        <child link = "inner_gimbal" />
        <origin rpy="0 0 0" xyz = "0 0 0" />
        <axis xyz = "0 1 0" />
    </joint>

    <joint name="inner_to_core" type="continuous">
        <parent link = "inner_gimbal" />
        <child link = "core" />
        <origin rpy="0 0 0" xyz = "0 0 0" />
        <axis xyz = "1 0 0" />
    </joint>



</robot>

